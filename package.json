{
  "name": "plussub",
  "version": "4.1.0",
  "description": "Chrome extension which adds subtitle to html5 videos",
  "author": "Stefan Breitenstein",
  "scripts": {
    "build:popup": "webpack --config src/popup/webpack.config.js --display-module",
    "build:background": "webpack --config src/background/webpack.config.js --display-module",
    "build:content": "webpack --config src/content/webpack.config.js --display-module",
    "build:option": "webpack --config src/option/webpack.config.js --display-module",
    "build": "npm run build:app && npm run build:background && npm run build:content && npm run build:content",
    "zip": "rm -f ../plussub.zip && bestzip plussub.zip manifest.json src/ res/",
    "test": "jest --no-cache",
    "version": "node sync-manifest.js && git add manifest.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/vue-fontawesome": "^0.1.10",
    "axios": "^0.18.0",
    "cssbeautify": "^0.3.1",
    "pako": "^1.0.6",
    "redux": "^4.0.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuetify": "^2.3.3"
  },
  "engine": "node >= 0.10",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-proposal-function-sent": "^7.10.4",
    "@babel/plugin-proposal-json-strings": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-numeric-separator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/plugin-proposal-throw-expressions": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "@fortawesome/fontawesome-free": "^5.13.1",
    "@types/jest": "^26.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.0.0",
    "bestzip": "^2.1.5",
    "css-loader": "^1.0.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.6",
    "vue-loader": "^15.4.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "prettier": {
    "trailingComma": "none",
    "printWidth": 200,
    "singleQuote": true
  }
}
